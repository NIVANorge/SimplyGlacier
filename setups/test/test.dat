# Saved from model : "simplyq_svalbard_model.txt"

data_set {
	
"""
Very basic test setup that you can modify
"""
	
	time_step([day])
	
	series("test_series.xlsx")
	
	patch : index_set("Patch") [ "Mountainside" "Glacier upper" "Glacier lower" "Plain" ]
	
	sc : index_set("Subcatchment") [ "Valley" ]
	
	connection("Downstream") {
		
		r : compartment("River", sc)
		
		directed_graph [ 
			r[ "Valley" ] -> out
		]
	}
	
	connection("Downhill throughflow") {
		
		s : compartment("Soil", patch)
		r : compartment("River")
		
		directed_graph [ 
			s[ "Mountainside" ] -> s[ "Glacier upper" ] -> s[ "Glacier lower" ] -> s[ "Plain" ] -> r[ ]
		]
	}
	
	connection("Downhill glacier") {
		
		g : compartment("Glacier", patch)
		r : compartment("River")
		
		directed_graph [ 
			g[ "Glacier upper" ] -> g[ "Glacier lower" ] -> r[ ]
		]
	}
	
	par_group("System") {
		
		par_datetime("Start date") 
		[ 2004-01-01 ]
		
		par_datetime("End date") 
		[ 2010-12-31 ]
	
	}
	
	par_group("Catchment data", sc) {
		
		par_real("Catchment area") 
		[ 51.7 ]
		
		par_real("Elevation of input met data") 
		[ 100 ]
	
	}
	
	par_group("Patch data", sc, patch) {
		
		par_real("Relative area") [
			0.2 0.2 0.2 0.2 
		]
	
	}
	
	module("ArcticSnow", version(0, 0, 1)) {
		
		par_group("Snow") {
			
			par_real("Degree-day factor for snowmelt") 
			[ 4 ]
			
			par_real("Refreeze efficiency") 
			[ 0 ]
			
			par_real("Liquid water fraction") 
			[ 0 ]
			
			par_real("Initial snow depth (water equivalents)") 
			[ 0 ]
			
			par_real("Snow fall correction factor") 
			[ 1 ]
			
			par_real("Snow melt quickflow fracn") 
			[ 0 ]
		
		}
	
	}
	
	module("Degree-day PET", version(1, 0, 0)) {
		
		par_group("Potential evapotranspiration", patch) {
			
			par_real("Degree-day factor for evapotranspiration") 
			[ 0.16 0 0 0.16 ]
			
			par_real("Minimal temperature for evapotranspiration") 
			[ 0 0 0 0 ]
		
		}
	
	}
	
	module("SimplyQ land", version(0, 5, 1)) {
		
		par_group("Hydrology general") {
			
			par_real("Baseflow index") 
			[ 0.6 ]
			
			par_real("Quick flow inflection point") 
			[ 400 ]
			
			par_real("Permanent wilting point as fraction of field capacity") 
			[ 0.5 ]
		
		}
		
		par_group("Hydrology land", patch) {
			
			par_real("Field capacity") 
			[ 20 100 100 200 ]
			
			par_real("Soil water time constant") 
			[ 2 8 8 8 ]
		
		}
	
	}
	
	module("SimplyQ river", version(0, 5, 0)) {
		
		par_group("Reach parameters", sc) {
			
			par_real("Reach slope") 
			[ 0.014 ]
			
			par_real("Reach length") 
			[ 10000 ]
			
			par_real("Manning's roughness coefficient") 
			[ 0.04 ]
			
			par_real("Initial reach flow") 
			[ 0.5 ]
		
		}
	
	}
	
	module("SimplyQ groundwater", version(0, 5, 0)) {
		
		par_group("Groundwater") {
			
			par_real("Groundwater time constant") 
			[ 30 ]
			
			par_real("Groundwater retention volume") 
			[ 0 ]
		
		}
		
		par_group("Groundwater extraction", sc) {
			
			par_real("Groundwater extraction") 
			[ 0 ]
		
		}
	
	}
	
	module("SimplyQ river trapezoid", version(0, 1, 0)) {
		
		par_group("Reach parameters", sc) {
			
			par_real("Reach slope") 
			[ 0.014 ]
			
			par_real("Reach bank slope") 
			[ 1 ]
			
			par_real("Reach bottom width") 
			[ 2 ]
			
			par_real("Reach length") 
			[ 10000 ]
			
			par_real("Manning's roughness coefficient") 
			[ 0.04 ]
			
			par_real("Initial reach flow") 
			[ 1 ]
		
		}
	
	}
	
	module("ArcticAtmosphere", version(0, 0, 1)) {
		
		par_group("Elevation data", sc, patch) {
			
			par_real("Average elevation") [
				1000 600 300 5 
			]
		
		}
		
		par_group("Temperature correction") {
			
			par_real("Temp lapse rate") 
			[ -0.004 ]
			
			par_real("Precip lapse rate") 
			[ 0.0019 ]
		
		}
	
	}
	
	module("SimplyGlacier", version(0, 0, 0)) {
		
		par_group("Glacier global") {
			
			par_real("Glacier percolation fraction") 
			[ 0.1 ]
			
			par_real("Degree-day factor for ice melt") 
			[ 3 ]
			
			par_real("Glacier accumulation coefficient") 
			[ 0 ]
			
			par_real("Rain ablation coefficient") 
			[ 80 ]
			
			par_real("Glacier time constant") 
			[ 1 ]
		
		}
		
		par_group("Glacier", sc, patch) {
			
			par_real("Initial ice thickness") [
				0 100000 100000 0 
			]
		
		}
	
	}
	
	module("SimplySed", version(0, 0, 0)) {
		
		par_group("Glacier sediments") {
			
			par_real("Production rate of mobilizable sediments") 
			[ 1 ]
			
			par_real("Mobilization rate") 
			[ 1 ]
			
			par_real("Mobilization rate exponent") 
			[ 2 ]
		
		}
	
	}

}